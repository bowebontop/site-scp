                                for z, AdminName in ipairs(Stuff) do
                                    if v.Name == AdminName then
                                        if v:FindFirstChildOfClass("Motor6D") then
                                            local Weld = v:FindFirstChildOfClass("Motor6D")
                                            Weld:Destroy()
                                        end
                                    end
                                end
                            end
                        end)
                    end)

